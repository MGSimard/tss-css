#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules/vinxi/bin/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules/vinxi/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules/vinxi/bin/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules/vinxi/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/vinxi@0.5.3_@types+node@22._3838804423eb60fcd2f3d3ede6c82df6/node_modules:/mnt/c/Users/DRKN/Desktop/MGSimard/Webdev/tss-css/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
